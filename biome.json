{
	"$schema": "https://biomejs.dev/schemas/2.1.1/schema.json",
	"files": {
		"includes": ["src/**", "*.ts", "*.tsx", "*.js", "*.jsx"]
	},
	"javascript": {
		"formatter": {
			"enabled": true
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true,
		"lineWidth": 120
	}
}
